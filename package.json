{
  "name": "wiremock-jp",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:frontend": "pnpm --filter @wiremock-jp/frontend dev",
    "dev:backend": "pnpm --filter @wiremock-jp/backend dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "db:generate": "pnpm --filter @wiremock-jp/backend db:generate",
    "test:e2e": "pnpm --filter @wiremock-jp/e2e test",
    "test:e2e:ui": "pnpm --filter @wiremock-jp/e2e test:ui",
    "test:e2e:headed": "pnpm --filter @wiremock-jp/e2e test:headed",
    "test:e2e:keep-data": "pnpm --filter @wiremock-jp/e2e test:keep-data",
    "test:e2e:keep-data:headed": "pnpm --filter @wiremock-jp/e2e test:keep-data:headed"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
